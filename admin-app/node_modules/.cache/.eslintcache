patch, useSelector } from 'react-redux'\nimport { Redirect } from 'react-router-dom'\nimport Layout from '../../layouts/header.layout'\n\nconst Signin = (props) => {\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('')\n  const [error, setError] = useState('')\n\n  const auth = useSelector((state) => state.auth)\n  const dispatch = useDispatch()\n\n  const handleChange = (e) => {\n    e.preventDefault()\n\n    const user = {\n      email,\n      password,\n    }\n    dispatch(login(user))\n  }\n\n  if (auth.authenticate) {\n    return <Redirect to={'/'} />\n  }\n\n  return (\n    <Layout sidebar>\n      <Container>\n        <Row style={{ marginTop: '50px' }}>\n          <Col md={{ span: 6, offset: 3 }}>\n            <Form onSubmit={handleChange}>\n              <Form.Group controlId=\"email\">\n                <Form.Label>Email Address</Form.Label>\n                <Form.Control\n                  type=\"Email\"\n                  placeholder=\"Enter Email\"\n                  value={email}\n                  onChange={(e) => setEmail(e.target.value)}\n                ></Form.Control>\n\n                <Form.Group onSubmit={handleChange}>\n                  <Form.Label>Password</Form.Label>\n                  <Form.Control\n                    type=\"Password\"\n                    placeholder=\"Enter Password\"\n                    value={password}\n                    onChange={(e) => setPassword(e.target.value)}\n                  ></Form.Control>\n                </Form.Group>\n              </Form.Group>\n              <Button type=\"submit\" variant=\"primary\">\n                Sign In\n              </Button>\n            </Form>\n          </Col>\n        </Row>\n      </Container>\n    </Layout>\n  )\n}\n\nexport default Signin\n","/home/yazkh/Documents/WebDevelopment/ZingVibesWeb/admin-app/src/components/container/signup/Signup.container.js",["167","168","169"],"import React, { useState } from 'react'\nimport { Button